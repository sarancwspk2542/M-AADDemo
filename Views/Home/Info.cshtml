@{
    ViewData["Title"] = "Infomation";
}
<h2 style="color:white" center>Infomation</h2>
<div style="color:white">
    <table cellpadding="2" cellspacing="2">
        <tr>
            <th>Claim</th>
            <th>Value</th>
        </tr>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
</div>